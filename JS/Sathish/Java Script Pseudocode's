Grade calculator
    let score = prompt("Enter the number")
    while(isNaN(score)== true){
        input=(prompt("invalid  input please enter the valid input"))
    }
    Marks= Number(score)
    if (Marks>0 && Marks<101){
        if(Marks >=90 && Marks<=100){
            console.log("Grade is A")
        }
        else if (Marks>=80 && Marks<=89){
            console.log("Grade is B")
        }
        else if (Marks>=70 && Marks<=79){
            console.log("Grade is C")
        } 
        else if (Marks>=60 && Marks<=69){
            console.log("Grade id D")
        }
        else if(Marks<60){
            console.log("Grade is Fail")
        }
    }
    else{
        console.log("Enter the number range betweeen the range of 0 to 100")
    }

Guessing Game
debugger;
    let G = prompt("select the number between 1 to 10")
    let num = 6
    while(isNaN(G)== true){
        G=prompt("Invalid input please enter the valid input")
    }
    while(G>=0 && G<=10 == false){
        G=prompt("Please enter the number between 1 to 10")
    }
    GG= Number(G)
        if(GG> num ){
            console.log("Too high")
        }
        else if(GG< num){
            console.log("Too Low")
        }
        else{
            console.log("Your Gussed number is right!")
        }

Pattern
    let n = 5

    for (let i=1; i<= n; i++) {
        let star=""
        for (let j=1; j<=i; j++) {
            star += "*"
        }
        console.log(star)
    }

Simple Calculator
debugger;
    let n1 = prompt("enter the first number")
    let n2 = prompt("enter the second number")
    let oper = prompt("enter the operator")
    let sol
    while(isNaN(n1)== true || isNaN(n2)== true){
        n1=prompt("Invalid input Please enter the valid input(n1)")
        n2=prompt("Invalid input Please enter the valid input(n2)")
        oper=prompt("Please enter the valid operator")
    }
    let firstnumber =Number(n1)
    let secondnumber = Number(n2)
    if (oper =="+"){
        sol=firstnumber+secondnumber
        console.log(sol)
    }
    else if(oper =="-"){
        sol=firstnumber-secondnumber
        console.log(sol)
    }
    else if(oper =="*"){
        sol=firstnumber*secondnumber
        console.log(sol)
    }
    else if(oper=="/"){
        sol= firstnumber/secondnumber
        console.log(sol)
    }

sum of digits until the digit remains one digit
debugger;
    let o = prompt("enter the number")
    while(isNaN(o)==true){
        o=prompt("Invalid input please enter the valid input")
    }
    let x = Number(o)
    let digit, sum = 0 
    while(x!= 0){
        digit = x%10 
        sum =sum+digit 
        x = Math.floor(x/10)
    }
    let y =Number(sum)
    let sum1
    while (y!=0){
        let digit1 = y%10
        sum1 = Number(sum1+digit1)
        y= Math.floor(y/10)
    }
    console.log("Sum of all digits number=", sum1)

Count of 3 appears in the given number
    let a = prompt("enter the number")
    while (isNaN(a) == true){
        input = prompt("invalid input please enter the correct input:")
    }
    let b = Number(a)
    let count = 0
    for (let i=1 ;i <=b ; i++){
        let digit = i
        while( digit != 0){
            if( digit % 10 == 3){ 
            count ++
            }
            digit=Math.floor(digit/ 10)
        }	
    }
    console.log("The count of 3 in the given number=", count)

Product and sum of the digits
    let p = prompt("enter the number")
    while (isNaN(p)== true){
        p=prompt("Invalid input please enter the valid input")
    }
    let n= Number(p)
    let digit, product=1, sum=0
    while( n!= 0 ){
        digit = n%10
        product = product*digit
        sum = sum + digit
        n = Math.floor(n/10)
    }
    let diff = product - sum
    console.log("Sum of the digits=",sum, "Product of the digits", product)

sum of even and odd numbers
    let e = prompt("Enter the number");
    while (isNaN(e)){
        e = prompt("Invalid input! Please enter a valid number:")
    }
    let num=Number(e)
    let evensum=0, oddsum = 0
    while (num>0){
        let digit=num%10
        if (digit%2==0) {
            evensum += digit;
        } else {
            oddsum+= digit;
        }
        num = Math.floor(num/10)
    }
    console.log("Sum of even digits =", evensum)
    console.log("Sum of odd digits =", oddsum)

Perfect square and even number
debugger;
    let s = prompt("enter the number")
    while (isNaN(s) == true){
        s=prompt("invalid input please enter the correct input:")
    }
    let n = Number(s)
    if( n%2==0 && Math.sqrt(n)*Math.sqrt(n) == n){
        console.log('Given is a perfect square and even number')
    }
    else{
        console.log('Given is not valid')
    }

