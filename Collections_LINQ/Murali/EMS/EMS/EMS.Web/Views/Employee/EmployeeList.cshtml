@model List<EMS.Models.EmployeeModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{

}
<table class="table table-bordered">
	<thead class="bg-black text-white">
		<tr>
			<th>Addresses</th>
			<th>Employeecode</th>
			<th>FirstName</th>
			<th>LastName</th>
			<th>DepartmentIdFk</th>
			
		</tr>
	</thead>
	<tbody>
		@foreach (var emp in Model)
		{
			<tr>
			<td>@emp.Addresses</td>
			<td>@emp.Employeecode</td>
			<td>@emp.FirstName</td>
			<td>@emp.LastName</td>	
			<td>@emp.DepartmentIdFk</td>
			</tr>
		}
	</tbody>
</table>

