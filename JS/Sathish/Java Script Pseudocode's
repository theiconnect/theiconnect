even or odd
    let x = prompt("enter the input:")
    if(x % 2 == 0){
        console.log(x,"is even")
    }
    else{
        console.log(x,"is odd")
    }

largest of three number

    let a = prompt("enter the first number:")
    let b = prompt("enter the second number:")
    let c = prompt("enter the third number:")

    if(a == b && b ==c && c == a){
        console.log("all are equal")
    }
    else{
        if(a >= b && a >= c){
            console.log(a,"is a largest Number")
        }
        else if(b >= a && b >= c){
            console.log(b,"is a largest Number")
        }
        else if(c >= a && c >= b){
            console.log(c,"is a largest Number")
        }
    }

sum of 1 to N

    let n = prompt("enter the range:")
    let sum = 0
    for(let i=0; i <= n ; i++){
        sum = sum + i
    }
    console.log(sum)

factorial of a number

    let f = prompt("Enter the number:")
    let fact = 1
    for(let i=1;i <= f ; i++){
        fact = fact * i
    }
    console.log(fact)

prime number

    let r = prompt("enter the number:")
    let count = 0
    for(let i = 1; i <= r ; i++){
        if(r % i == 0){
            count++
        }
    }
    if(count == 2){
        console.log(r,"is the prime number")
    }
    else{
        console.log(r,"is not a prime")
    }


