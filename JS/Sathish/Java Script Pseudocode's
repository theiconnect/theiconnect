reverse of a number

    let q =prompt("enter the number:")
    while(isNaN(q)== true){
        	input = prompt("invalid input please enter the correct input:")
    }
    let d= Number(q)
    let digit,rev=0
    while( d != 0){
        digit = d % 10
        rev = rev * 10 + digit
        d = Math.floor(d / 10)
    }
    console.log(rev,'is the reverse of number')

palinderome

    let m=prompt("Enter a number")
    while(isNaN(m)==true){
        input= prompt("invalid inputk please enter the correct input")
    }
    input = m
    let digits,pal=0
    while(m!=0){
        digits=m % 10
        pal=pal*10+digits
        m= Math.floor(m/10)
    }
    if(pal==input){
        console.log("It is a Palindrome Number")
    }
    else{
        console.log("It is not a Planindrome Number")
    }

count of given numbers
    debugger;
    let a = prompt("enter the number")
    while (isNaN(a) == true){
        input = prompt("invalid input please enter the valid input")    
    }
    b= Number(a)
    let digit,count=0
    while( b != 0 ){
        digit = b % 10
        b = Math.floor(b / 10)
        count++
    }
    console.log("Count of given digits =",count)

